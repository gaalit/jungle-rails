
<header class="page-header">
<h1 id="header" class="header-sign-up">Login</h1>
<header>

<%= form_tag '/login' do %>



<div class="form-group">
Email: <%= text_field_tag :email %>
</div>

<div class="form-group">
Password: <%= password_field_tag :password %>
</div>



<div class="btn-group">
  <%= submit_tag "Login" %>
</div>

<% end %>
